{
    "name": "ptmdevelopment/mollie_interface",
    "description": "A mollie api wrapper for subscriptions and events, based on Mollie php api, Mollie laravel api and Mollie cashier.",
    "authors": [
        {
            "name": "Jonathan Peetoom",
            "email": "jonathan@ptmdevelopment.net"
        }
    ],
    "license": "MIT",
    "homepage": "https://github.com/JonathanPTM/mollie-interface",
    "require": {
        "mollie/mollie-api-php": "^2.27",
        "illuminate/support": "^8.0|^9.0|^10",
        "illuminate/database": "^8.0|^9.0|^10",
        "nesbot/carbon": "^2.31",
        "mollie/laravel-mollie": "^2.19.1|^2.20|^2.21",
        "moneyphp/money": "^3.3|^4.0|^4.1",
        "imtigger/laravel-job-status": "^1.2"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "laravel/framework": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "PTM\\MollieInterface\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PTM\\MollieInterface\\MollieInterfaceServiceProvider"
            ],
            "aliases": {
                "PTM": "PTM\\MollieInterface\\PTMFacade"
            }
        }
    }
}
